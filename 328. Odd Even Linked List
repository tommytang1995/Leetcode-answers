    public ListNode oddEvenList(ListNode head) {
        if(head==null||head.next==null||head.next.next==null)return head;
        ListNode odd = head;
        ListNode even = odd.next;
        ListNode ehead = even;
        while(even!=null&&even.next!=null){
            odd.next=odd.next.next;
            even.next=even.next.next;
            odd=odd.next;
            even=even.next;
        }
        odd.next=ehead;
        return head;
    }
