public class Solution {
    public int hammingDistance(int x, int y) {
        int res = 0;
        while(x != 0 || y != 0){
            int bit1 = (x == 0 ? 0 : x % 2);
            int bit2 = (y == 0 ? 0 : y % 2);
            if(bit1 != bit2) res++;
            x /= 2;
            y /= 2;
        }
        return res;
    }
}
