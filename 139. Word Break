public class Solution {
    public boolean wordBreak(String s, Set<String> wordDict) {
        boolean[] bool = new boolean[s.length()+1];
        bool[0] = true;
        for(int i = 1 ; i <= s.length() ; i++){
            for(int j = 0 ; j < i ; j++){
                if(bool[j]&&wordDict.contains(s.substring(j,i))){
                    bool[i]=true;
                    break;
                }
            }
        }
        return bool[s.length()];
    }
}
