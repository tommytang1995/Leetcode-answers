

public class Solution {
    public String reverseWords(String s) {
        if(s.length()==0||s==null)return "";
        String[] str = s.split(" ");
        StringBuilder sb = new StringBuilder();
        for(int i = 0 ; i < str.length ; i++){
            if(!str[i].isEmpty()){
                sb.insert(0,str[i]);
                if(i<str.length-1){
                    sb.insert(0," ");
                }
            }
        }
        return sb.toString();
    }
}
